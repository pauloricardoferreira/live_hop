        <jsScript_script>//Script here

var DAG =
	"import pendulum"
+ '\n' + "from datetime import datetime, timedelta"
+ '\n' + "from airflow import DAG"
+ '\n' + "from airflow_hop.operators import HopWorkflowOperator"
+ '\n' + ""
+ '\n' + "local_tz=pendulum.timezone('America/Sao_Paulo')"
//+ '\n' + "local_tz.convert(logical_date)"
+ '\n' + ""
+ '\n' + "default_args = {"
+ '\n' + "    'owner': 'Agroterenas',"
+ '\n' + "    'depends_on_past': False,"
+ '\n' + "    'start_date': datetime.today() - timedelta(days=1),  #datetime(2021, 3, 13, 0, tzinfo=local_tz), datetime(yyyy,mm,dd,hh,mn,sc, tzinfo=local_tz),"
+ '\n' + "    'email': ['admin@gmail.com'],"
+ '\n' + "    'email_on_failure': False,"
+ '\n' + "    'email_on_retry': False,"
+ '\n' + "    'retries': 0,"
+ '\n' + "    'retry_delay': timedelta(seconds=30)"
+ '\n' + "}"
+ '\n' + ""
+ '\n' + "dag = DAG("
+ '\n' + "    dag_id='" + DAG_ID + "',"
+ '\n' + "    default_args=default_args,"
+ '\n' + "    schedule_interval='" + SCHEDULER_INTERVAL + "',"
+ '\n' + "    catchup=False,"
+ '\n' + "    tags=[" + TAGS + "]"
+ '\n' + ")"
+ '\n' + ""
+ '\n' + "job = HopWorkflowOperator("
+ '\n' + "    dag=dag,"
+ '\n' + "    task_id='" + TASK_ID + "',"
+ '\n' + "    workflow='" + JOB_NAME + ".hwf',"
+ '\n' + "    project_name='LIVE',"
+ '\n' + "    log_level= '" + LEVEL + "'"
+ '\n' + ")"</jsScript_script>
      </jsScript>
    </jsScripts>
